@using WellaMates.Models
@model WellaMates.Models.RefundItemCategory

@switch (Model)
{
    case RefundItemCategory.MEAL:
        <img src="~/Content/images/empty.png" class="icons-inverted" style="background-position: -140px -70px" title="Refeição"/>
        break;
    case RefundItemCategory.TELEPHONE:
        <img src="~/Content/images/empty.png" class="icons-inverted" style="background-position: 0 -70px" title="Telefonia"/>
        break;
    case RefundItemCategory.TRANSPORTATION:
        <img src="~/Content/images/empty.png" class="icons-inverted" style="background-position: -70px -70px" title="Transporte"/>
        break;
    case RefundItemCategory.OTHER:
        <img src="~/Content/images/empty.png" class="icons-inverted" style="background-position: -210px -70px" title="Outros"/>
        break;
    case RefundItemCategory.XEROX_COPY:
        <img src="~/Content/images/empty.png" class="icons-inverted" style="background-position: 0 -140px" title="Outros"/>
        break;
    case RefundItemCategory.MAIL_SEDEX:
        <img src="~/Content/images/empty.png" class="icons-inverted" style="background-position: -280px -140px" title="Correio/Sedex"/>
        break;
    case RefundItemCategory.LUGGAGE:
        <img src="~/Content/images/empty.png" class="icons-inverted" style="background-position: -350px -140px" title="Excesso de Bagagem"/>
        break;
}